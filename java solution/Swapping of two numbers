import java.util.Scanner;
public class Swap {
    public static void main(String args[]) {
        int x=10, y=20,temp;
        System.out.println("Enter the Number");
       
        
        System.out.println(x+","+y);
        temp = x;
        x=y;
        y=temp;
        
        System.out.println(x+","+y);
        
        
    }
}




Swapping of Number from 


import java.util.Scanner;
public class Swap {
    public static void main(String args[]) {
        int x, y,temp;
        System.out.println("Enter the Number");
       Scanner read = new Scanner(System.in);
        x = read.nextInt();
        y = read.nextInt();
        
        System.out.println(x+","+y);
        temp = x;
        x=y;
        y=temp;
        
        System.out.println(x+","+y);
        
        
    }
}
